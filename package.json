{
  "name": "kycell",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/blueprint",
    "packages/engine",
    "packages/cli",
    "packages/ai"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "lint": "eslint packages/*/src/",
    "play": "tsx packages/cli/src/cli.ts",
    "compile": "node packages/blueprint/dist/cli.js",
    "web": "pnpm --filter @kycell/web dev",
    "web:build": "pnpm --filter @kycell/web build"
  },
  "keywords": [
    "text-game",
    "immersive-sim",
    "natural-language"
  ],
  "author": "",
  "license": "MIT",
  "description": "A CLI for creating immersive sim text games using natural language object definitions",
  "dependencies": {
    "@kycell/blueprint": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
